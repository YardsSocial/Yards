{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "allowJs": true,
    "checkJs": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    "**/*.jsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/tabs/sign-in.jsx",
    "app/auth/sign-in.jsx",
    ".expo/_layout.tsx",
    "app/tabs/NewDM.js",
    "app/index.js",
    "components/UIComponents/PaymentInput.js",
    "components/UIComponents/UploadPixels.js"
  ]
}
